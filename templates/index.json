ily: Anton, sans-serif;\n    font-weight: 500;\n    text-transform: uppercase;\n  }\n\n  .gta-timer__separator {\n    color: #ffda3a;\n    padding-bottom: 5px;\n  }\n\n  .gta-coupon {\n    font-family: Anton, sans-serif;\n    font-size: 17px;\n    font-weight: 600;\n    text-transform: uppercase;\n    color: #000000;\n    padding: 12px 16px;\n    margin: 0;\n    background-color: #ffe4e8;\n    border: 1px dashed #ee6293;\n    border-radius: 4px;\n    gap: 4.25px;\n    cursor: pointer;\n  }\n\n  @media screen and (max-width: 768px) {\n    .gta-content__container {\n      padding: 16px 20px;\n    }\n\n    .gta-content__text {\n      font-size: 32px;\n    }\n\n    .gta-timer__unit-value {\n      font-size: 60px;\n    }\n\n    .gta-coupon {\n      font-size: 12px;\n      padding: 6px 12px;\n    }\n  }\n</style>\n\n<div class=\"gta-content__container\">\n  <div class=\"gta-content__overlay\"></div>\n  <div class=\"gta-content\">\n    <div class=\"gta-content__text\">{{ offer_title }}</div>\n    <div class=\"gta-content__description\">{{ offer_description }}</div>\n    <div class=\"gta-content__timer\">\n      <div class=\"gta-timer__wrapper\">\n        <div class=\"gta-timer__unit\">\n          <h4 class=\"gta-timer__unit-value\" data-timer-days>00</h4>\n          <div class=\"gta-timer__unit-label\">Jours</div>\n        </div>\n        <div class=\"gta-timer__separator\">:</div>\n        <div class=\"gta-timer__unit\">\n          <h4 class=\"gta-timer__unit-value\" data-timer-hours>19</h4>\n          <div class=\"gta-timer__unit-label\">Heures</div>\n        </div>\n        <div class=\"gta-timer__separator\">:</div>\n        <div class=\"gta-timer__unit\">\n          <h4 class=\"gta-timer__unit-value\" data-timer-minutes>22</h4>\n          <div class=\"gta-timer__unit-label\">Minutes</div>\n        </div>\n        <div class=\"gta-timer__separator\">:</div>\n        <div class=\"gta-timer__unit\">\n          <h4 class=\"gta-timer__unit-value\" data-timer-seconds>08</h4